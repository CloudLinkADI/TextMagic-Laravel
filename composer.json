{
  "name": "cloudlinkadi/textmagic-laravel",
  "type": "package",
  "description": "A Laravel wrapper for the TextMagic PHP SMS API.",
  "keywords": ["textmagic", "sms", "api","laravel-package, cloudlink"],
  "homepage": "https://github.com/CloudLinkADI/TextMagic-Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Coghlan",
      "email": "info@cloud.link",
      "homepage": "http://cloud.link"
    },
    {
      "name": "Sunny Kashyap",
      "email": "kashyap.sunny679@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.*",
    "textmagic/sdk":"*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
    "psr-4": {
      "CloudLinkADI\\TextMagic\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CloudLinkADI\\TextMagic\\TextMagicServiceProvider"
      ],
      "aliases": {
        "TextMagic": "CloudLinkADI\\TextMagic\\TextMagicFacade"
      }
    }
  }
}
